# Makefile for sequential game-of-life benchmark
#
# @configure_input@
#

SHELL =		/bin/sh

SMLNJ =		@SMLNJ_CMD@
MLTON =		@MLTON@
PMLC =		@PMLC@
#PMLCFLAGS =	-Cdebug=true

TARGET =	life-seq

all:		$(TARGET)

$(TARGET):	life-seq.mlb life-seq.pml
	$(PMLC) $(PMLCFLAGS) -Csequential=true -o $(TARGET) life-seq.mlb

.PHONY: clean
clean:
	rm -f $(TARGET) *.s
