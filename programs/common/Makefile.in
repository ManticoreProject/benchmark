# Makefile for common benchmark utilities
#
# @configure_input@
#

SHELL =		/bin/sh

SMLNJ =		@SMLNJ_CMD@
MLTON =		@MLTON@
PMLC =		@PMLC@
PMLCFLAGS =	@PMLCFLAGS@

all:		gen-random-int-list

gen-random-int-list:	gen-random-int-list.sml gen-random-int-list.cm 
	echo "SMLofNJ.exportFn(\"gen-random-int-list\", Main.main);" | $(SMLNJ) gen-random-int-list.cm
	sml @SMLload=gen-random-int-list 1000000 file > random-int-list.txt

.PHONY: clean
clean:
	rm -f random-int-list.txt
