# Makefile for pfib benchmarks
#
# @configure_input@
#

SHELL =		/bin/sh

SMLNJ =		@SMLNJ_CMD@
MLTON =		@MLTON@
PMLC =		@PMLC@
PMLCFLAGS=@PMLCFLAGS@

TARGET =	pfib

all:		$(TARGET) $(TARGET)-seq-elison $(TARGET)-smlnj

$(TARGET):	pfib.mlb pfib.pml
	$(PMLC) $(PMLCFLAGS) -o $(TARGET) pfib.mlb

$(TARGET)-seq-elison:	pfib.mlb pfib.pml
	$(PMLC) $(PMLCFLAGS) -Csequential=true -o $(TARGET)-seq-elison pfib.mlb

$(TARGET)-smlnj:	fib.cm fib.sml
	echo "SMLofNJ.exportFn(\"fib\", Main.main);" | $(SMLNJ) fib.cm

.PHONY: clean
clean:
	rm -f $(TARGET) $(TARGET)-seq-elison
