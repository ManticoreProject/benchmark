# Makefile for fib-array benchmarks
#
# @configure_input@
#

SHELL =		/bin/sh

SMLNJ =		@SMLNJ_CMD@
MLTON =		@MLTON@
PMLC =		@PMLC@
#PMLCFLAGS =	-Cdebug=true

TARGET =	fib-array

all:		$(TARGET)-par $(TARGET)-seq-elison

$(TARGET)-par:	fib-array.mlb fib-array.pml
	$(PMLC) $(PMLCFLAGS) -o $(TARGET)-par fib-array.mlb

$(TARGET)-seq-elison:	fib-array.mlb fib-array.pml
	$(PMLC) $(PMLCFLAGS) -Csequential=true -o $(TARGET)-seq-elison fib-array.mlb

.PHONY: clean
clean:
	rm -f $(TARGET)-par $(TARGET)-seq-elison *.s
