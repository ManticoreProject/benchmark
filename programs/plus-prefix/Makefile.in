# Makefile for plus-prefix benchmark
#
# @configure_input@
#

SHELL =		/bin/sh

SMLNJ =		@SMLNJ_CMD@
MLTON =		@MLTON@
PMLC =		@PMLC@
PMLCFLAGS =	@PMLCFLAGS@

TARGET =	plus-prefix

all:		$(TARGET)

$(TARGET):	plus-prefix.mlb plus-prefix.pml
	$(PMLC) $(PMLCFLAGS) -Csequential=true -o $(TARGET) plus-prefix.mlb

.PHONY: clean
clean:
	rm -f $(TARGET) *.s
