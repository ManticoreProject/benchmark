# Makefile for the vproc-pingpong benchmark
#
# @configure_input@
#

SHELL =		/bin/sh

PMLC =		@PMLC@
PMLCFLAGS =	@PMLCFLAGS@

TARGET =	vproc-pingpong

all:		$(TARGET) $(TARGET)-trunk

$(TARGET):	$(TARGET).pml $(TARGET).mlb
	$(PMLC) $(PMLCFLAGS) -o $(TARGET) $(TARGET).mlb

$(TARGET)-trunk:	$(TARGET).pml $(TARGET)-trunk.mlb
	$(PMLC) $(PMLCFLAGS) -o $(TARGET)-trunk $(TARGET)-trunk.mlb

.PHONY: clean
clean:
	rm -f $(TARGET) $(TARGET)-trunk
