#!/bin/bash

NAME="list-mergesort"
DESCRIPTION="mergesort over a linked list"

## Parameters provided by the configure script

# number of times to test each data point; for statistical validity
NUM_TRIALS="@NUM_TRIALS@"
MANTI_BENCH_ROOT=@MANTI_BENCH_ROOT@

# sequential elison 
$MANTI_BENCH_ROOT/scripts/experiment.sh -e -f list-mergesort -n $NAME -d "$DESCRIPTION" -t $NUM_TRIALS 

# smlnj
$MANTI_BENCH_ROOT/scripts/experiment-smlnj.sh -f list-mergesort-smlnj -n "$NAME" -d "$DESCRIPTION" -t $NUM_TRIALS

# mlton
$MANTI_BENCH_ROOT/scripts/experiment-mlton.sh -f list-mergesort-mlton -n "$NAME" -d "$DESCRIPTION" -t $NUM_TRIALS
