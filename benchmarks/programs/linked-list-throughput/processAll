echo -n "" > results.txt

for f in results/*; do
    cores=$( ls $f | grep -o "[0-9]\+")
    
    output="$(./../../scripts/processFile.py $f)"

    throughput="$(echo "$output" | grep -o "Total throughput.*[0-9]\+" | grep -o "[0-9]\+\.[0-9]\+")"

    echo -n "$cores " >> results.txt
    echo $throughput >> results.txt
    
done    

 
