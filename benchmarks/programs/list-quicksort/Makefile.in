# Makefile for pquicksort benchmarks
#
# @configure_input@
#

SHELL =		/bin/sh

SMLNJ =		@SMLNJ_CMD@
MLTON =		@MLTON@
PMLC =		@PMLC@
PMLCFLAGS=@PMLCFLAGS@

TARGET =	list-quicksort

all:		$(TARGET)  $(TARGET)-smlnj $(TARGET)-mlton

$(TARGET):	list-quicksort.mlb list-quicksort.pml
	$(PMLC) $(PMLCFLAGS) -Csequential=true -o $(TARGET) list-quicksort.mlb

$(TARGET)-smlnj:	list-quicksort.cm list-quicksort.sml
	echo "SMLofNJ.exportFn(\"list-quicksort-smlnj\", Main.main);" | $(SMLNJ) list-quicksort.cm

$(TARGET)-mlton:	list-quicksort-mlton.mlb list-quicksort.pml
	$(MLTON) -output $(TARGET)-mlton list-quicksort-mlton.mlb

.PHONY: clean
clean:
	rm -rf $(TARGET) $(TARGET)-mlton *.s .cm *.x86-linux
