# Makefile for spawn benchmarks
#
# @configure_input@
#

SHELL =		/bin/sh

SMLNJ =		@SMLNJ_CMD@
MLTON =		@MLTON@
PMLC =		@PMLC@

all:		spawn-smlnj spawn-mc1 spawn-mc2

spawn-smlnj:	sources.cm spawn.sml
	echo "SMLofNJ.exportFn(\"spawn-smlnj\", Main.main);" | $(SMLNJ) sources.cm

spawn-mc1:	spawn1.pml
	$(PMLC) -o spawn-mc1 spawn1.pml

spawn-mc2:	spawn2.pml
	$(PMLC) -o spawn-mc2 spawn2.pml
