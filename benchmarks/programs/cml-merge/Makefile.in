# Makefile for primes benchmarks
#
# @configure_input@
#

SHELL =		/bin/sh

SMLNJ =		@SMLNJ_CMD@
MLTON =		@MLTON@
PMLC =		@PMLC@

TARGET =	merge

all:		$(TARGET)-smlnj $(TARGET)-mc1 $(TARGET)-mc2

$(TARGET)-smlnj:	sources.cm merge.sml
	echo "SMLofNJ.exportFn(\"merge-smlnj\", Main.main);" | $(SMLNJ) sources.cm

$(TARGET)-mc1:	merge1.pml
	$(PMLC) -o $(TARGET)-mc1 merge1.pml

$(TARGET)-mc2:	merge2.pml
	$(PMLC) -o $(TARGET)-mc2 merge2.pml

