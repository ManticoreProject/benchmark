#!/bin/bash

NAME="plus-prefix"
DESCRIPTION="sequential plus prefix computation over a list"

## Parameters provided by the configure script

# number of times to test each data point; for statistical validity
NUM_TRIALS="@NUM_TRIALS@"
MANTI_BENCH_ROOT=@MANTI_BENCH_ROOT@

# sequential
$MANTI_BENCH_ROOT/scripts/experiment.sh -e -f plus-prefix -n $NAME -d "$DESCRIPTION" -t $NUM_TRIALS 

# smlnj
$MANTI_BENCH_ROOT/scripts/experiment-smlnj.sh -f plus-prefix-smlnj -n "$NAME" -d "$DESCRIPTION" -t $NUM_TRIALS

# mlton
$MANTI_BENCH_ROOT/scripts/experiment-mlton.sh -f plus-prefix-mlton -n "$NAME" -d "$DESCRIPTION" -t $NUM_TRIALS
