
if [ "$#" -ne 1 ]; then
    echo "usage: ./mkData <size>"
    exit
fi

n=$1

echo $n > data/data.txt

m="$(( $n * 5 ))"

for (( c=1; c<=$m; c++ ))
do
   x=$(( RANDOM % $n ))
   echo $x >> data/data.txt
   x=$(( RANDOM % $n ))
   echo $x >> data/data.txt
   x=$(( RANDOM % $n ))
   echo $x >> data/data.txt
   x=$(( RANDOM % $n ))
   echo $x >> data/data.txt
   
done


