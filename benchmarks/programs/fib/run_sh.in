#!/bin/bash

NAME="pfib"
DESCRIPTION="naive fibonacci calculation"

## Parameters provided by the configure script

# number of times to test each data point; for statistical validity
NUM_TRIALS="@NUM_TRIALS@"
MANTI_BENCH_ROOT=@MANTI_BENCH_ROOT@
# generate different processor configurations used by the experiment
# e.g., -p 1 -p 2 ... -p num_procs
PROCS_LIST=""
for i in {1..@NUM_PROCS@}; do
    PROCS_LIST="$PROCS_LIST -p $i"
done

# parallel 
$MANTI_BENCH_ROOT/scripts/experiment.sh $PROCS_LIST -f pfib -n $NAME -d "$DESCRIPTION" -t $NUM_TRIALS

# sequential elison 
$MANTI_BENCH_ROOT/scripts/experiment.sh -e -f pfib-seq-elison -n $NAME -d "$DESCRIPTION" -t $NUM_TRIALS 

# smlnj
$MANTI_BENCH_ROOT/scripts/experiment-smlnj.sh -f fib-smlnj -n "fib" -d "$DESCRIPTION" -t $NUM_TRIALS

# mlton
$MANTI_BENCH_ROOT/scripts/experiment-mlton.sh -f fib-mlton -n "fib" -d "$DESCRIPTION" -t $NUM_TRIALS
